<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta content="IE=Edge chrome=1" http-equiv="X-UA-Compatible">
    <meta content="width=device-width initial-scale=1.0" name="viewport">
    <title>Discuss-It</title>
    <!-- searchability -->
    <meta content="Discuss It - Online discussion tracker for finding conversations" name="description">
    <meta content="lucas charles" name="author">
    <meta content="index" name="robots">
    <meta content="website discussions reddit slashdot hackernews conversations search" name="keywords">
    <meta content="Discuss It" property="og:title">
    <meta content="Website" property="og:type">
    <meta content="http://discussitapp.com" property="og:url">
    <meta content="Discuss It App" property="og:site_name">
    <meta content="Discuss It - Online discussion tracker for finding conversations" property="og:description">
    <!-- mobile specific metas -->
    <meta content="width=device-width initial-scale=1" name="viewport">
    <!-- CSS stylesheets -->
    <link rel="stylesheet" href="/css/app.css">
    <!-- Google Search Console -->
    <meta content="kMG6lgDLRhhYRIaPnIyu-Zu-Ll8O463w9V6cvzPTDZ4" name="google-site-verification">
    <!-- Google Analytics -->
    <script type="text/javascript">
        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
        (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
        m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
        })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
        ga('create', 'UA-29308675-3', 'auto');
        ga('send', 'pageview');
    </script>
  </head>
  <body>
    <nav class="navbar navbar-default navbar-fixed-top">
      <div class="container-fluid">
        <!-- Brand and toggle get grouped for better mobile display -->
        <div class="navbar-header">
          <button class="navbar-toggle collapsed" aria-expanded="false" data-target="#navbar-collapsable-1" data-toggle="collapse" type="button">
            <span class="sr-only ">Toggle navigation</span>
            <span class="icon-bar">
            </span>
            <span class="icon-bar">
            </span>
            <span class="icon-bar">
            </span>
          </button>
          <a class="navbar-brand" href="/">Discuss It!</a>
        </div>
        <!-- Collect the nav links, forms, and other content for toggling -->
        <div id="navbar-collapsable-1" class="collapse navbar-collapse">
          <ul class="nav navbar-nav">
            <li>
              <a href="/about">About</a>
            </li>
          </ul>
        </div>
        <!--.navbar-collapse>  <![endif]-->
      </div>
      <!--.container-fluid>  <![endif]-->
    </nav>
    <div class="container-fluid">
      <div class="row">
        <div class="col-md-1">
        </div>
        <div class="col-md-10">
          <%= render @view_module, @view_template, assigns %>
        </div>
      </div>
    </div>
    <footer>
      <p class="col-sm-offset-1 app-version">Discuss It 2016 - v<%= @app_version %></p>
      <!-- <br> -->
    </footer>
    <script src="<%= "#{static_path(@conn, "/js/app.js")}" %>"></script>
  </body>
</html>
